citHeader("To cite india in publications use:")

bibentry(bibtype = "Manual",
  title = "Tools for Influence Diagnostics in Statistical Models",
  author = as.person("F. Osorio"),
  year = "2023",
  note = "R package version 0.1",
  url  = "https://github.com/faosorios/india",
  textVersion =
    paste("Osorio, F. (2023).",
      "Tools for Influence Diagnostics in Statistical Models.",
      "R package version 0.1.", "URL: https://github.com/faosorios/india")
)
