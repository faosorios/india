citHeader("To cite india in publications use:")

bibentry(bibtype = "Manual",
  title = "india: Influence Diagnostics in Statistical Models",
  author = as.person("F. Osorio"),
  year = "2026",
  note = "R package version 0.1-2",
  url  = "https://github.com/faosorios/india",
  textVersion =
    paste("Osorio, F. (2026).",
      "india: Influence Diagnostics in Statistical Models.",
      "R package version 0.1-2.", "URL: https://github.com/faosorios/india")
)
